#cloud-config
package_update: true
packages:
  - qemu-guest-agent
runcmd:
  - systemctl enable --now qemu-guest-agent.service
  - sh /root/startup.sh
ssh_authorized_keys:
  - ${ssh_authorized_key}
write_files:
- encoding: b64
  content: ${startup}
  owner: root:root
  path: /root/startup.sh
  permissions: '0644'